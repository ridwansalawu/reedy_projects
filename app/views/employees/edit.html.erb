<%= link_to("<< Back to All Employees", employees_path, :class => 'back-link') %>
<h1>Edit <%= @employee.first_name.upcase + ", " + @employee.last_name %></h1>
<div class="employees new">
  <h2>Edit a New Employee</h2>

  <%= form_for @employee do |f| %>

    <%= render(:partial => "form", :locals => {:f => f}) %>


    <div class="form-buttons">
      <%= f.submit("Update Employee") %>
    </div>

  <% end %>
</div>
